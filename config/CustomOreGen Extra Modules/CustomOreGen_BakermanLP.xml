<ConfigSection>
    <IfCondition condition=':= dimension.generator = "RandomLevelSource"'>
    <ConfigSection>
        <Substitute name='StandardOreSubstitute' block='stone'>
            <Description>
                Replace vanilla-generated ore clusters with stone.
            </Description>
            <Comment>
                The global option deferredPopulationRange must be large enough to catch all ore clusters
            </Comment>
            <Replaces block='oreCoal'/>
            <Replaces block='oreIron'/>
            <Replaces block='oreGold'/>
            <Replaces block='oreRedstone'/>
            <Replaces block='oreDiamond'/>
            <Replaces block='oreLapis'/>
            <Replaces block='oreEmerald'/>
        </Substitute>
        <Substitute name='IC2Substitute' block='stone'>
            <Description>
                Replace IC2-generated ore clusters with stone.
            </Description>
            <Comment>
                The global option deferredPopulationRange must be large enough to catch all ore clusters
            </Comment>
            <Replaces block='blockOreCopper'/>
            <Replaces block='blockOreTin'/>
            <Replaces block='blockOreUran'/>
            <Replaces block='blockOreLead'/>
        </Substitute>
        <Substitute name='ForestrySubstitute' block='stone'>
            <Description>
                Replace Forestry-generated ore clusters with stone.
            </Description>
            <Comment>
                The global option deferredPopulationRange must be large enough to catch all ore clusters
            </Comment>
            <Replaces block='apatite:0'/>
            <Replaces block='apatite:1'/>
            <Replaces block='apatite:2'/>
        </Substitute>
    	<Substitute name='certusQuartzSubstitute' block='stone'>
            <Replaces block='1032:0' />
	    </Substitute>
        <Substitute name='pamsHcSaltSubstitute' block='stone'>
            <Replaces block='2201:0'/>
        </Substitute>
        <Substitute name='TConstructAluminumSubstitute' block='stone'>
            <Replaces block='1548:5'/>
        </Substitute>
    </ConfigSection>

    <ConfigSection>
        <Veins name='TConstructAluminumVeins' block='1548:5' branchType='Bezier'>
            <Description>
                Beschreibung
            </Description>
            
            <DrawWireframe>:=drawWireframes</DrawWireframe>
            <WireframeColor>0xFFFF00FF</WireframeColor>

            <Replaces block='stone'/>
            <Replaces block='1007:0'/>
            <Replaces block='dirt' weight='0.5'/>
            <Replaces block='gravel' weight='0.5'/>
            <Biome name='.*'/>

            <Setting name='MotherlodeFrequency' avg=':= 16 * 0.025 * oreFreq'/>
            <Setting name='MotherlodeSize' avg=':= 0' range=':= 0'/>
            <Setting name='MotherlodeHeight' avg=':= 48/64 * dimension.groundLevel' range=':= 8/64 * dimension.groundLevel' type='normal'/>

            <Setting name='BranchFrequency' avg='1' range='0'/> 
            <Setting name='BranchInclination' avg='0' range='0.6'/> 
            <Setting name='BranchLength' avg='32' range='16'/> 
            <Setting name='BranchHeightLimit' avg='16'/> 

            <Setting name='SegmentForkFrequency' avg='0'/>  
            <Setting name='SegmentLength' avg='16' range='8'/> 
            <Setting name='SegmentAngle' avg='0.40' range='0.40'/> 
            <Setting name='SegmentRadius' avg=':= 0.5 * oreSize' range=':= 0.3 * oreSize'/>

            <Setting name='OreDensity' avg='1' range='0'/> 
            <Setting name='OreRadiusMult' avg='1.0' range='0.1'/> 
        </Veins>
    <Cloud name='aeCertusQuartzCloud' block='1032:0' inherits='PresetStrategicCloud'>
        <DrawWireframe>:=drawWireframes</DrawWireframe>
        <WireframeColor>0xFFDDA0DD</WireframeColor>
        <Setting name='DistributionFrequency' avg=':= 4 * _default_'/>
        <Setting name='CloudRadius' avg=':= 1.2 * _default_' range=':= 1.2 * _default_'/>
        <Setting name='CloudThickness' avg=':= 0.5 * _default_' range=':= 0.5 * _default_'/>                
        <Setting name='OreVolumeNoiseCutoff' avg='0'/>
        <Setting name='OreDensity' avg=':= 1.1 * _default_'/>
        <Veins name='aeCertusQuartzVeins' block='1032:0' inherits='PresetHintVeins'>
            <DrawWireframe>:=drawWireframes</DrawWireframe>
            <WireframeColor>0xFFDDA0DD</WireframeColor>
            <Setting name='MotherlodeFrequency' avg=':= _default_' range=':= _default_'/>   
            <Setting name='MotherlodeRangeLimit' avg=':= 0.5 * _default_' range=':= 0.5 * _default_'/>              
        </Veins>
        </Cloud>

<Veins name='CoalVeins' block='oreCoal' inherits='PresetLayeredVeins'>
    <Description>
      Coal
        - Hauptvorkommen 40 - 72
        - keine Hauptvorkommen
        - Erzadern 16 - 96
        - nur eine Erzader
        - Erzadern starten im Bereich der Hauptvorkommen
    </Description>
    <DrawWireframe>:=drawWireframes</DrawWireframe>
    <WireframeColor>0x60101010</WireframeColor>

    <Setting name='MotherlodeFrequency' avg=':= 16 * 0.025 * oreFreq'/>
    <Setting name='MotherlodeSize' avg=':= 0' range=':= 0'/>
    <Setting name='MotherlodeHeight' avg=':= 56/64 * dimension.groundLevel' range=':= 16/64 * dimension.groundLevel' type='normal'/>

    <Setting name='BranchFrequency' avg='1' range='0'/> 
    <Setting name='BranchInclination' avg='0' range='0'/> 
    <Setting name='BranchLength' avg='64' range='32'/> 
    <Setting name='BranchHeightLimit' avg=':= 16/64 * dimension.groundLevel' range=':= 8/64 * dimension.groundLevel'/> 

    <Setting name='SegmentForkFrequency' avg='0'/>  
    <Setting name='SegmentLength' avg='16' range='8'/> 
    <Setting name='SegmentAngle' avg='0.40' range='0.40'/> 
    <Setting name='SegmentRadius' avg=':= 1 * oreSize' range=':= 0.4 * oreSize'/>

    <Setting name='OreDensity' avg='1' range='0'/> 
    <Setting name='OreRadiusMult' avg='1.0' range='0.1'/> 

    <Replaces block='stone'/>
    <Replaces block='projectred.exploration.stone:0' weight='0.8'/><Comment>Project Red: Marble</Comment>
    <Replaces block='projectred.exploration.stone:3' weight='0.8'/><Comment>Project Red: Basalt</Comment>
    <Replaces block='dirt' weight='0.5'/>
    <Replaces block='gravel' weight='0.5'/>
  </Veins>

  <Veins name='RedstoneVeins' block='73' inherits='PresetVerticalVeins'>
    <Description>
      Redstone
        - senkrechte Erzadern
        - starten zwischen 24 - 40
    </Description>
    <DrawWireframe>:=drawWireframes</DrawWireframe>
    <WireframeColor>0x40FF0000</WireframeColor>

    <Setting name='MotherlodeFrequency' avg=':= 20 * 0.025 * oreFreq'/>
    <Setting name='MotherlodeSize' avg=':= 0' range=':= 0'/>
    <Setting name='MotherlodeHeight' avg=':= 32/64 * dimension.groundLevel' range=':= 10/64 * dimension.groundLevel' type='normal'/>

    <Setting name='BranchFrequency' avg='1' range='0'/> 
    <Setting name='BranchInclination' avg='-1.31' range='0.22'/> 
    <Setting name='BranchLength' avg=':= 32/64 * dimension.groundLevel' range=':= 10/64 * dimension.groundLevel'/> 
    <Setting name='BranchHeightLimit' avg=':= 50/64 * dimension.groundLevel' range=':= 0'/> 

    <Setting name='SegmentForkFrequency' avg='0.1'/>  
    <Setting name='SegmentLength' avg='16' range='8'/> 
    <Setting name='SegmentAngle' avg='0' range='0.3'/> 
    <Setting name='SegmentRadius' avg=':= 0.5 * oreSize' range=':= 0.1 * oreSize'/>

    <Setting name='OreDensity' avg='1' range='0'/> 
    <Setting name='OreRadiusMult' avg='1.0' range='0.1'/> 

    <Replaces block='stone'/>
    <Replaces block='projectred.exploration.stone:0' weight='0.8'/><Comment>Project Red: Marble</Comment>
    <Replaces block='projectred.exploration.stone:3' weight='0.8'/><Comment>Project Red: Basalt</Comment>
    <Replaces block='dirt' weight='0.5'/>
    <Replaces block='gravel' weight='0.5'/>
  </Veins>
